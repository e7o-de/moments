{
	"router": {
		"class": "\\e7o\\Moments\\Request\\Routers\\SimpleRouter"
	},
	"error": {
		"template": "error.htm"
	},
	"routes": [],
	"services": {
		"database": {
			"class": "\\e7o\\Moments\\Database\\Connection",
			"args": [
				"%database"
			]
		},
		"template_loader": {
			"class": "\\e7o\\Moments\\Output\\Template\\MomentsLoader",
			"args": [
				"${root}/views",
				"${moments}/views"
			]
		},
		"template": {
			"class": "\\e7o\\Morosity\\Morosity",
			"args": [
				"@template_loader"
			]
		},
		"forms": {
			"class": "\\e7o\\Moments\\Output\\Forms\\Generator",
			"args": [
				"@template",
				"${root}/forms"
			]
		}
	}
}
